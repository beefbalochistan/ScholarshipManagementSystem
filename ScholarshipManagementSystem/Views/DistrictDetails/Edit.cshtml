@model ScholarshipManagementSystem.Models.Domain.MasterSetup.DistrictDetail

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/_Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>DistrictDetail</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DistrictDetailId" />
            <div class="form-group">
                <label asp-for="DistrictId" class="control-label"></label>
                <select asp-for="DistrictId" class="form-control" asp-items="ViewBag.DistrictId"></select>
                <span asp-validation-for="DistrictId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Population" class="control-label"></label>
                <input asp-for="Population" class="form-control" />
                <span asp-validation-for="Population" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MaleRatio" class="control-label"></label>
                <input asp-for="MaleRatio" class="form-control" />
                <span asp-validation-for="MaleRatio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FemaleRatio" class="control-label"></label>
                <input asp-for="FemaleRatio" class="form-control" />
                <span asp-validation-for="FemaleRatio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CensesYear" class="control-label"></label>
                <input asp-for="CensesYear" class="form-control" />
                <span asp-validation-for="CensesYear" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GrowthRate" class="control-label"></label>
                <input asp-for="GrowthRate" class="form-control" />
                <span asp-validation-for="GrowthRate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

