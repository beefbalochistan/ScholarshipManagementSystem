@model IEnumerable<DAL.Models.Domain.Student.ApplicantCurrentStatus>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/_Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProcessState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProcessState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ApplicantCurrentStatusId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ApplicantCurrentStatusId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ApplicantCurrentStatusId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
