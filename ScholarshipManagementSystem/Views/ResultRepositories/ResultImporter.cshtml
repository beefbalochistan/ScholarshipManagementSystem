@section Scripts {
    <partial name="_ValidationScriptsPartial" />
    <!-- twitter-bootstrap-wizard js -->
    <script src="~/assets/libs/bs-custom-file-input/bs-custom-file-input.min.js"></script>
    <script src="~/assets/js/pages/form-element.init.js"></script>
    <script src="~/assets/libs/toastr/build/toastr.min.js"></script>
    <script src="~/assets/libs/twitter-bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/assets/libs/twitter-bootstrap-wizard/prettify.js"></script>
    <script src="~/assets/libs/select2/js/select2.min.js"></script>
    <script src="~/assets/js/pages/form-advanced.init.js"></script>
    
    <script>
        var IsClean = false;
        var IsResultExist = false;
        var CurrentTab = 0;
        var IsCriteriaApplied = false;
        var IsMeritListGenerated = false;
        $(document).ready(function () {           
            $('#progrss-wizard').bootstrapWizard({
                tabClass: 'nav nav-pills',
                onNext: function (tab, navigation, index) {
                    if (index == 1) {
                        if ($("#SchemeId").val() == 0) {
                            notificationme('Please select Scheme!');
                            $('#schemeDiv').removeClass().addClass('display-4 mdi mdi-progress-alert text-danger');
                            onPrevious(tab, navigation, 1);
                        }
                    } else if (index == 2) {
                        if ($("#SchemeLevelId").val() == 0) {
                            notificationme('Please select fill required fields!');
                            $("#schemeLevelDiv").show();
                            $('#schemeLevelDiv').removeClass().addClass('display-4 mdi mdi-progress-alert text-danger');
                            onPrevious(tab, navigation, 2);
                        } else if ($("#SchemeId").val() >= 4 && $("#DegreeScholarshipLevelId").val() == 0) {
                            notificationme('Please select fill required fields!');
                            $("#schemeLevelDiv").show();
                            $('#schemeLevelDiv').removeClass().addClass('display-4 mdi mdi-progress-alert text-danger');
                            onPrevious(tab, navigation, 2);
                        }
                        if (($("#SchemeLevelId").val() == 4 || $("#SchemeLevelId").val() == 5 || $("#SchemeLevelId").val() == 6) && $("#DAEInstituteId").val() == 0) {
                            notificationme('Please select fill required fields!');
                            $("#schemeLevelDiv").show();
                            $('#schemeLevelDiv').removeClass().addClass('display-4 mdi mdi-progress-alert text-danger');
                            onPrevious(tab, navigation, 2);
                        }
                        $.ajax({
                            url: '@Url.Action("GetResultStatus", "SelectionCriteriaGenerals")',
                            data: {
                                PolicySRCForumId: $('#PolicySRCForumId').val(),
                                SchemeLevelId: $('#SchemeLevelId').val(),
                                SchemeId: $('#SchemeId').val(),
                                DegreeScholarshipLevelId: $('#DegreeScholarshipLevelId').val(),
                                DAEInstituteId: $('#DAEInstituteId').val()
                            },
                            success: function (data) {
                                $('#RRId').val(data.rrid);
                                if (data.success) {
                                    IsResultExist = true;
                                    IsClean = data.isclean;
                                    IsCriteriaApplied = data.iscriteriaapplied;
                                    IsMeritListGenerated = data.ismeritlistgenerated;
                                    $('#btnImportResult').prop('disabled', true);
                                    $('#btnAttachResult').prop('disabled', true);
                                    if (data.ismeritlistgenerated) {
                                        $('#btnGenerateMeritList').prop('disabled', true);
                                        notificationme("Result Already Imported!");
                                        $('#progrss-wizard').bootstrapWizard('show', 6);
                                        CurrentTab = 6;
                                    } else if (data.iscriteriaapplied) {
                                        $('#btnApplyCriteria').prop('disabled', true);
                                        $('#btnAssistDocument').prop('disabled', true);
                                        $('#progrss-wizard').bootstrapWizard('show', 6);
                                        CurrentTab = 6;
                                    } else if (data.isclean) {
                                        $('#btnAssistDocument').prop('disabled', true);
                                        $('#progrss-wizard').bootstrapWizard('show', 5);
                                        fetchSelectionCriteriaGeneral();
                                        notificationme('Selection criteria defined against selected Scheme Level!');
                                        CurrentTab = 5;
                                    } else if (data.isclean == false) {
                                        $('#progrss-wizard').bootstrapWizard('show', 4);
                                        fetchAssistDocumentGeneral();
                                        notificationme('Document Assist Indicators defined against selected Scheme Level!');
                                        CurrentTab = 4;
                                    }
                                    $('#previousLink').removeClass().addClass("disabled-link");
                                } else {
                                    $('#btnImportResult').prop('disabled', false);
                                    $('#btnApplyCriteria').prop('disabled', false);
                                    $('#btnAssistDocument').prop('disabled', false);
                                    $('#btnAttachResult').prop('disabled', false);
                                    $('#btnGenerateMeritList').prop('disabled', false);
                                    $('#previousLink').removeClass();
                                }
                                $(".analize").hide();
                            }
                        });
                    } else if (index == 3) {
                        var excelFile = document.getElementById('ExcelFile');
                        var pdfFile = document.getElementById('PdfFile');
                        if (excelFile.files.length == 0 || pdfFile.files.length == 0) {
                            notificationme("Please attach required file!");
                            onPrevious(tab, navigation, 3);
                        } else {
                            AttachFile();
                        }
                    }
                    else if (index == 4) {
                        if ($("#RRId").val() == "0") {
                            notificationme("Please import result!");
                            onPrevious(tab, navigation, 4);
                        }
                        //fetchAssistDocumentIndicator(0);
                        fetchAssistDocumentGeneral();
                        notificationme('Document Assist Indicators defined against selected Scheme Level!');
                    }
                    else if (index == 5) {
                        @*if (IsClean == false) {
                            notificationme('Please assist document before proceed!');
                            onPrevious(tab, navigation, 4);
                        }*@
                        fetchSelectionCriteriaGeneral();
                        notificationme('Selection criteria defined against selected Scheme Level!');
                    } else if (index == 6) {
                        if (IsCriteriaApplied == false) {
                            notificationme('Please apply criteria before proceed!');
                            onPrevious(tab, navigation, 5);
                        }
                    }
                    //$('#progrss-wizard').bootstrapWizard('show', 3);
                },
                onPrevious: function (tab, navigation, index) {
                    if (IsResultExist) {
                        @*if (IsCriteriaApplied) {
                            $('#progrss-wizard').bootstrapWizard('show', 6);
                        } else if (IsClean) {
                            $('#progrss-wizard').bootstrapWizard('show', 5);
                        } else if (!IsClean) {
                            $('#progrss-wizard').bootstrapWizard('show', 4);
                        }*@
                        onNext(tab, navigation, (index+1));
                    }
                },
                onFinish: function (tab, navigation, index) {
                    alert(index);
                    //$('#progrss-wizard').bootstrapWizard('show', 3);
                },
            });
        });

        $(document).ready(function () {
            @*$("#isReuploadDiv").hide();*@
            $("#PolicySRCForumId").val(0);
            $("#SchemeLevelId").val(0);
            $("#SchemeId").val(0);
            $("#PolicySRCForumId").change(function () {
                var PolicySRCForumId = $("#PolicySRCForumId").val();
                $(".TempScholarshipFiscalYear").val($("#PolicySRCForumId option:selected").text());
                var SchemeId = $("#SchemeId").val();
                if (SchemeId != 0) {
                    if (PolicySRCForumId != 0) {
                        $("#SchemeLevelId").empty();
                        $.get("/ResultAnalyzer/GetSchemeLevels", { policySRCForumId: PolicySRCForumId,schemeId: SchemeId }, function (data) {
                            $("#SchemeLevelId").empty();
